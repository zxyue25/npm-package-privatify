{
    "name": "npm-package-privatify",
    "version": "1.1.4",
    "description": "一个将私有源npm包处理为离线包的自动化工具",
    "main": "./build",
    "scripts": {
        "clear": "rimraf build",
        "tsc": "npm run clear && tsc",
        "p": "npm run tsc && npm publish",
        "test": "node scripts/test.ts",
        "lint": "tslint ./src/**/*.ts --fix",
        "build": "npm install && rimraf build/ && tsc --build ./tsconfig.json"
    },
    "bin": {
        "privatify": "./bin.js"
    },
    "files": [
        "build",
        "bin.js",
        "private-server-boilerplate"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zxyue25/npm-package-privatify.git"
    },
    "author": "zxyue25",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/zxyue25/npm-package-privatify/issues"
    },
    "homepage": "https://github.com/zxyue25/npm-package-privatify#readme",
    "dependencies": {
        "@babel/preset-typescript": "^7.14.5",
        "chalk": "^4.1.1",
        "commander": "^8.0.0",
        "compressing": "^1.5.1",
        "execa": "^5.1.1",
        "fs-extra": "^10.0.0",
        "globby": "^11.0.4",
        "handlebars": "^4.7.7",
        "inquirer": "^8.1.1",
        "minimatch": "^3.0.4",
        "ora": "^5.4.1"
    },
    "devDependencies": {
        "@types/jest": "^26.0.22",
        "jest": "^26.6.3",
        "minimist": "^1.2.5",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.5.4",
        "ts-node": "^10.0.0",
        "tslint": "^5.1.0",
        "typescript": "^4.2.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "ts"
        ],
        "testMatch": [
            "<rootDir>/src/__tests__/*.spec.ts"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/"
        ],
        "transform": {
            "^.+\\.ts$": "<rootDir>/node_modules/ts-jest"
        },
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/__tests__/",
            "/dist/"
        ],
        "testTimeout": 50000
    },
    "engines": {
        "node": ">=7.6"
    },
    "keywords": [
        "npm",
        "privatify"
    ]
}
